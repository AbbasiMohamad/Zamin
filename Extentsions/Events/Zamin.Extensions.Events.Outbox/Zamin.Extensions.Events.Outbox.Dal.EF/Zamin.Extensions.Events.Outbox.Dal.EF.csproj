<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<Version>7.0.0-beta.2</Version>
		<Nullable>enable</Nullable>
		<Authors>Alireza Oroumand</Authors>
		<ImplicitUsings>enable</ImplicitUsings>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<RepositoryUrl>https://github.com/oroumand/Zamin</RepositoryUrl>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="7.0.0" />
		<PackageReference Include="Zamin.Core.Domain" Version="7.3.0-beta.1" />
		<PackageReference Include="Zamin.Extensions.Events.Abstractions" Version="7.0.0-beta.1" />
		<PackageReference Include="Zamin.Extensions.Serializers.Abstractions" Version="7.0.0-beta.1" />
		<PackageReference Include="Zamin.Extensions.UsersManagement.Abstractions" Version="7.0.0-beta.1" />
		<PackageReference Include="Zamin.Infra.Data.Sql.Commands" Version="7.2.0-beta.1" />
	</ItemGroup>
	<ItemGroup>
		<Using Include="Microsoft.EntityFrameworkCore" />
	</ItemGroup>
</Project>
